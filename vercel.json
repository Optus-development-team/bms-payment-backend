{
  "version": 3,
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs20.x",
      "memory": 2048,
      "maxDuration": 120,
      "includeFiles": "dist/**"
    }
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "routes": [
    { "src": "/docs(.*)", "dest": "/api/index.ts" },
    { "src": "/v1/(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/api/index.ts" }
  ]
}
